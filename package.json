    "isomorphic-fetch": "^2.2.1",
    "json5": "^2.0.1",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "lodash": "^4.17.11",
    "marked": "^0.5.0",
    "pdfmake": "^0.1.38"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "7.0.1",
    "@babel/node": "^7.0.0",
