{
  "version": "0.9.14",
  "description": "The easiest to use pdf template editor and renderer. Even non-techs can edit!",
  "main": "build/node/index.js",
  "browser": "build/browser/browser.js",
  "bin": {
    "runtemplate": "build/node/bin/runtemplate.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "clean": "del build",
    "prebuild": "npm run clean",
    "build": "npm-run-all build:*",
    "build:node": "BABEL_ENV=node babel src --ignore 'test','**/*.test.js' --out-dir build/node",
    "build:browser": "BABEL_ENV=browser babel src --ignore 'test','**/*.test.js' --out-dir build/browser",
    "dev": "run-p dev:*",
    "dev:node": "npm run build:node -- --watch",
    "dev:browser": "npm run build:browser -- --watch",
    "prepublishOnly": "npm run build && npm test",
    "test": "NODE_ENV=test jest"
  },
  "dependencies": {
    "core-js": "3.1.4",
    "date-fns": "1.30.1",
    "fast-json-patch": "^2.1.0",
    "fs-extra": "^8.0.1",
    "got": "9.6.0",
    "isomorphic-fetch": "^2.2.1",
    "json-e-customizable": "^3.0.1",
    "keyv-lru-files": "1.0.2",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "lodash": "^4.17.11",
    "markdown-it": "^8.4.2",
    "mem": "5.1.0",
    "path-parse": "^1.0.6",
    "pdfmake": "0.1.57",
    "raw-body": "2.4.0",
    "regenerator-runtime": "0.13.2"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-plugin-lodash": "3.3.4",
    "del-cli": "^2.0.0",
    "delay": "4.3.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "22.7.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-lodash": "^5.1.0",
    "jest": "24.8.0",
    "jest-fetch-mock": "^2.1.2"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runtemplate/runtemplate.git"
  },
  "author": "Eric Fong",
  "keywords": [
    "bill",
    "pdf",
    "template",
    "render",
    "generate"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/runtemplate/runtemplate/issues"
  },
  "homepage": "https://github.com/runtemplate/runtemplate#readme",
  "name": "runtemplate"
}
