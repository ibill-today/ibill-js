  "browser": "./browser/index.js",
  "bin": {
    "ibill": "bin/ibill.js"
  },
  "scripts": {
    "dev": "rollup --config tool/rollup.config.js --silent --watch",
    "build": "rollup --config tool/rollup.config.js --silent",
